    <validationRules>
        <fullName>Prevent_Admission_on_Saturday_or_Sunday</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
  MOD( AdmissionDate__c - DATE(1900, 1, 7), 7 ) = 0,   /* Sunday */
  MOD( AdmissionDate__c - DATE(1900, 1, 7), 7 ) = 6    /* Saturday */
)</errorConditionFormula>
        <errorMessage>Admission Date cannot be on a Saturday or Sunday.</errorMessage>
    </validationRules>
